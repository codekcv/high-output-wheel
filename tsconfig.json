{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "ES2018",
    "outDir": "dist",
    "lib": ["esnext", "dom"],
    // "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "."
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
